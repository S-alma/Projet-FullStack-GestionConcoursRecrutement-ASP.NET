@model IEnumerable<WebApplication.Models.Inscription>

@{
    ViewBag.Title = "Index";
}

<br>
<h1><strong>Liste des inscriptions</strong></h1>
<br>

<p>
    @Html.ActionLink("Revenir à la connexion", "Connect")
</p>
<br>
<table class="table table-bordered">
    <tr>
        <th>
            <strong> Nom </strong>
        </th>
        <th>
            <strong> Prénom </strong>
        </th>
        <th>
            <strong> Numéro CIN </strong>
        </th>
        <th>
            <strong> Téléphone </strong>
        </th>
        <th>
            <strong> Date de naissance </strong>
        </th>
        <th>
            <strong> Lieu de naissance </strong>
        </th>
        <th>
            <strong> Code postal </strong>
        </th>
        <th>
            <strong> Sexe </strong>
        </th>
        <th>
            <strong> Ville </strong>
        </th>
        <th>
            <strong> Date de dépot </strong>
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Nom)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Prénom)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Numéro_CIN)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Téléphone)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Date_naissance)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Lieu_naissance)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.code_postal)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Sexe)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Ville)
    </td>
    <th>
        @Html.DisplayFor(modelItem => item.Date_dépot)
    </th>
    <td>
        @Html.ActionLink("Editer", "Edit", new { id = item.ID_inscription }) |
        @Html.ActionLink("Détail", "Details", new { id = item.ID_inscription }) |
        @Html.ActionLink("Supprimer", "Delete", new { id = item.ID_inscription })
    </td>
</tr>
    }

</table>
